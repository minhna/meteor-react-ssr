{
  "name": "server-render",
  "private": true,
  "scripts": {
    "start": "meteor run"
  },
  "dependencies": {
    "autoprefixer": "^7.2.2",
    "babel-runtime": "^6.26.0",
    "meteor-node-stubs": "^0.3.2",
    "postcss-easy-import": "^3.0.0",
    "postcss-import": "^11.0.0",
    "postcss-nested": "^3.0.0",
    "postcss-scss": "^1.0.2",
    "postcss-simple-vars": "^4.1.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-helmet": "^5.2.0",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {},
  "postcss": {
    "plugins": {
      "postcss-easy-import": {
        "extensions": [
          ".css",
          ".scss",
          ".import.css"
        ],
        "prefix": "_"
      },
      "autoprefixer": {
        "browsers": [
          "last 2 versions"
        ]
      }
    },
    "parser": "postcss-scss"
  }
}
